{
  "workbench.statusBar.feedback.visible": false,
  "workbench.colorTheme": "Dracula",
  "editor.fontSize": 15,
  "editor.fontFamily": "Operator Mono SSm",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json",
          "entity.name.type.class.js"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "code-eol.style": {
    "color": "#666",
    "opacity": 1.0
  },
  "code-eol.newlineCharacter": "¬",
  "code-eol.returnCharacter": "⇠",
  "code-eol.crlfCharacter": "↵",
  "editor.minimap.enabled": false,
  "files.trimTrailingWhitespace": true,
  "workbench.startupEditor": "newUntitledFile",
  "terminal.integrated.fontFamily": "MesloLGS-RegularForPowerline",
  "ruby.format": "rubocop",
  "window.zoomLevel": 1,
  "terminal.integrated.fontSize": 14,
  "cSpell.diagnosticLevel": "Error",
  "cSpell.languageSettings": [
    {
      "languageId": "markdown",
      "ignoreRegExpList": [
        "/^\\s{4}.+$/gm"
      ]
    }
  ],
  "editor.scrollBeyondLastLine": false,
  "extensions.ignoreRecommendations": true,
  "editor.wordWrapColumn": 100,
  "editor.rulers": [
    80,
    100,
    120
  ],
  "editor.multiCursorModifier": "alt",
  "editor.tabSize": 2,
  "testExplorer.addToEditorContextMenu": true,
  "rubyTestExplorer.testFramework": "rspec",
  "rubyTestExplorer.logpanel": true,
  "ruby.intellisense": "rubyLocate",
  "ruby.interpreter.commandPath": "$HOME/.rbenv/shims/ruby",
  "ruby.lint": {
    "rubocop": {
      "lint": true,
      "rails": true
    }
  },
  "explorer.compactFolders": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.semanticHighlighting.enabled": true,
  "java.configuration.checkProjectSettingsExclusions": false,
  "tabnine.experimentalAutoImports": true,
  "workbench.editor.showTabs": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}