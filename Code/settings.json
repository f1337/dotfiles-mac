{
  "workbench.statusBar.feedback.visible": false,
  "workbench.colorTheme": "Dracula",
  "editor.fontSize": 15,
  "editor.fontFamily": "Operator Mono SSm",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json",
          "entity.name.type.class.js"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "code-eol.style": {
    "color": "#666",
    "opacity": 1.0
  },
  "code-eol.newlineCharacter": "¬",
  "code-eol.returnCharacter": "⇠",
  "code-eol.crlfCharacter": "↵",
  "editor.minimap.enabled": false,
  "files.trimTrailingWhitespace": true,
  "workbench.startupEditor": "newUntitledFile",
  "terminal.integrated.fontFamily": "MesloLGS-RegularForPowerline",
  "terminal.integrated.fontSize": 14,
  "cSpell.diagnosticLevel": "Error",
  "cSpell.languageSettings": [
    {
      "languageId": "markdown",
      "ignoreRegExpList": [
        "/^\\s{4}.+$/gm"
      ]
    }
  ],
  "editor.scrollBeyondLastLine": false,
  "extensions.ignoreRecommendations": true,
  "editor.wordWrapColumn": 120,
  "editor.rulers": [

    80,
    120
  ],
  "editor.multiCursorModifier": "alt",
  "editor.tabSize": 2,
  "testExplorer.addToEditorContextMenu": true,
  "rubyTestExplorer.testFramework": "rspec",
  "rubyTestExplorer.logpanel": true,
  "ruby.interpreter.commandPath": "$HOME/.asdf/shims/ruby",
  "ruby.lint": {
    "rubocop": {
      "useBundler": true,
      "lint": true,
      "rails": false
    }
  },
  "explorer.compactFolders": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.semanticHighlighting.enabled": true,
  "java.configuration.checkProjectSettingsExclusions": false,
  "tabnine.experimentalAutoImports": true,
  "workbench.editor.showTabs": true,
  "ruby.useBundler": true,
  "prettier.semi": false,
  "ruby.intellisense": "rubyLocate",
  "prettier.arrowParens": "avoid",
  "editor.formatOnPaste": true,
  "editor.formatOnSaveMode": "modifications",
  "editor.formatOnSave": true,
  "ruby.format": "rubocop",
  "todo.file.name": "*.todo",
  "cSpell.userWords": [
    "activestorage",
    "emdr",
    "ketamine",
    "neurotypical",
    "ptsd",
    "turbolinks",
    "workstreams"
  ],
  "cSpell.logLevel": "Warning",
  "security.workspace.trust.untrustedFiles": "open",
  "scss.lint.unknownAtRules": "ignore",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "todo.symbols.box": "- [ ]",
  "todo.symbols.cancelled": "- [-]",
  "todo.symbols.done": "- [x]"
}